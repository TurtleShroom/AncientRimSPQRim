<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/RoyalTitleDef[defName = "UniversalFermenter"]</xpath>
    <match Class="PatchOperationAdd">
      <xpath>Defs/ThingDef[defName = "UniversalFermenter"]/comps/li[@Class="UniversalProcessors.CompProperties_UniversalFermenter"]/products</xpath>
      <value>
        <li>
          <thingDef>AR_Wine</thingDef>
          <ingredientFilter>
            <thingDefs>
              <li>AR_Mash</li>
            </thingDefs>
          </ingredientFilter>
          <baseFermentationDuration>360000</baseFermentationDuration>
        </li>
      </value>
    </match>
  </Operation>
</Patch>