<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Food crops -->
  <ThingDef ParentName="PlantBase">
    <defName>AR_PlantArtichoke</defName>
    <label>artichoke plant</label>
    <description>A variety of a species of thistle cultivated as a food.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Items/Plants/ArtichokePlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <plant>
      <fertilitySensitivity>0.2</fertilitySensitivity>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>AR_RawArtichoke</harvestedThingDef>
      <harvestYield>15</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>3.67</growDays>
      <visualSizeRange>0.3~1.05</visualSizeRange>
      <harvestMinGrowth>0.85</harvestMinGrowth>
      <sowResearchPrerequisites>
        <li>AR_Project_Mediterranean_Culture</li>
      </sowResearchPrerequisites>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>AR_PlantGrape</defName>
    <label>grape plant</label>
    <description>A tasty fruit that could easily be fermented into wine. Delicious even when raw.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Items/Plants/GrapesPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <plant>
      <fertilitySensitivity>0.6</fertilitySensitivity>
      <growDays>4.4</growDays>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>AR_RawGrapes</harvestedThingDef>
      <harvestYield>10</harvestYield>
      <sowMinSkill>5</sowMinSkill>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <immatureGraphicPath>Items/Plants/GrapesPlantImmature</immatureGraphicPath>
      <visualSizeRange>0.3~1.05</visualSizeRange>
      <sowResearchPrerequisites>
        <li>AR_Project_Mediterranean_Culture</li>
      </sowResearchPrerequisites>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>AR_PlantGrapeWild</defName>
    <label>wild grape plant</label>
    <description>A tasty fruit, that could easy fermented into wine. Delicious even when raw.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Items/Plants/GrapesWild</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <plant>
      <fertilitySensitivity>0.6</fertilitySensitivity>
      <growDays>4.4</growDays>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>AR_RawGrapes</harvestedThingDef>
      <harvestYield>10</harvestYield>
      <topWindExposure>0.1</topWindExposure>
      <immatureGraphicPath>Items/Plants/GrapesWildImmature</immatureGraphicPath>
      <wildClusterWeight>100</wildClusterWeight>
      <visualSizeRange>1.3~2.0</visualSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <wildBiomes>
        <!-- RimWorld.Core -->
        <!-- VANILLA RENDITION -->
        <AridShrubland>0.15</AridShrubland>
        <TemperateForest>0.1</TemperateForest>
        <BorealForest>0.04</BorealForest>
        <Tundra>0.001</Tundra>
        <ColdBog>0.01</ColdBog>
        <TemperateSwamp>0.001</TemperateSwamp>
        <Desert>0.05</Desert>
        <ExtremeDesert>0.05</ExtremeDesert>
        <!-- ___________________ -->
        <!-- Mod Biomes -->
        <!-- ___________________ -->
        <!-- sarg.alphabiomes -->
        <!-- ALPHA BIOMES -->
        <AB_GallatrossGraveyard MayRequire="sarg.alphabiomes">0.15</AB_GallatrossGraveyard>
        <AB_GelatinousSuperorganism MayRequire="sarg.alphabiomes">0.001</AB_GelatinousSuperorganism>
        <AB_OcularForest MayRequire="sarg.alphabiomes">0.01</AB_OcularForest>
        <AB_RockyCrags MayRequire="sarg.alphabiomes">0.1</AB_RockyCrags>
        <AB_TarPits MayRequire="sarg.alphabiomes">0.1</AB_TarPits>
        <!-- ___________________ -->
        <!-- BiomesTeam.Oasis -->
        <!-- CHROMATIC OASIS -->
        <!-- ___________________ -->
        <!-- Mlie.AdvancedBiomes -->
        <!-- ADVANCED BIOMES -->
        <PoisonForest MayRequire="Mlie.AdvancedBiomes">0.001</PoisonForest>
        <Savanna MayRequire="Mlie.AdvancedBiomes">0.1</Savanna>
        <Volcano MayRequire="Mlie.AdvancedBiomes">0.01</Volcano>
        <Wasteland MayRequire="Mlie.AdvancedBiomes">0.25</Wasteland>
        <Wetland MayRequire="Mlie.AdvancedBiomes">0.01</Wetland>
        <!-- ___________________ -->
        <!-- zylle.MoreVanillaBiomes -->
        <!-- More Vanilla Biomes -->
        <ZBiome_AlpineMeadow MayRequire="zylle.MoreVanillaBiomes">0.08</ZBiome_AlpineMeadow>
        <ZBiome_CoastalDunes MayRequire="zylle.MoreVanillaBiomes">0.1</ZBiome_CoastalDunes>
        <ZBiome_Grasslands MayRequire="zylle.MoreVanillaBiomes">0.15</ZBiome_Grasslands>
        <ZBiome_Marsh MayRequire="zylle.MoreVanillaBiomes">0.01</ZBiome_Marsh>
        <ZBiome_Sandbar_NoBeach MayRequire="zylle.MoreVanillaBiomes">0.15</ZBiome_Sandbar_NoBeach>
        <!-- ___________________ -->
        <!-- Mlie.NaturesPrettySweet -->
        <!-- NATURE IS PRETTY SWEET -->
        <TKKN_VolcanicFlow MayRequire="Mlie.NaturesPrettySweet">0.001</TKKN_VolcanicFlow>
        <TKKN_RedwoodForest MayRequire="Mlie.NaturesPrettySweet">0.1</TKKN_RedwoodForest>
        <TKKN_Grasslands MayRequire="Mlie.NaturesPrettySweet">0.15</TKKN_Grasslands>
        <!-- PRAIRIE -->
        <TKKN_Savanna MayRequire="Mlie.NaturesPrettySweet">0.15</TKKN_Savanna>
        <!-- _________________ -->
        <!-- biomesteam.biomesislands -->
        <!-- BIOMES ISLANDS -->
        <!-- _________________ -->
        <!-- biomesteam.biomesislands -->
        <!-- BIOMES ISLANDS -->
        <BiomesIslands_Atoll MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_Atoll>
        <BiomesIslands_BorealIsland MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_BorealIsland>
        <BiomesIslands_DesertIsland MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_DesertIsland>
        <BiomesIslands_DesertArchipelago MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_DesertArchipelago>
        <BiomesIslands_TemperateIsland MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_TemperateIsland>
        <BiomesIslands_TemperateArchipelago MayRequire="biomesteam.biomesislands">0.1</BiomesIslands_TemperateArchipelago>
        <BiomesIslands_TropicalIsland MayRequire="biomesteam.biomesislands">0.15</BiomesIslands_TropicalIsland>
        <BiomesIslands_TropicalArchipelago MayRequire="biomesteam.biomesislands">0.16</BiomesIslands_TropicalArchipelago>
        <BiomesIslands_TundraIsland MayRequire="biomesteam.biomesislands">0.01</BiomesIslands_TundraIsland>
        <!-- ___________________ -->
        <!-- Heis.Edo.Themed.Expansion -->
        <!-- HISTORICALLY ACCURATE WEEABOO MOD -->
        <HSBambooForest MayRequire="Heis.Edo.Themed.Expansion">0.1</HSBambooForest>
        <!-- ___________________ -->
        <!-- reel.frozenforest -->
        <!-- WINTER TAIGA -->
        <WinterTaiga MayRequire="reel.frozenforest">0.001</WinterTaiga>
        <!-- ___________________ -->
        <!-- pphhyy.gulden -->
        <!-- FOREST OF GOLD -->
        <TemperateGuldenForest MayRequire="pphhyy.gulden">0.1</TemperateGuldenForest>
      </wildBiomes>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
  <!-- tree -->
  <ThingDef ParentName="TreeBase">
    <defName>AR_TreeOlive</defName>
    <label>olive tree</label>
    <altitudeLayer>Building</altitudeLayer>
    <description>A strong hardwood tree. Slow-growing, but its amount of fruits is higher than that of the cultivated small plants.</description>
    <graphicData>
      <texPath>Items/Plants/OliveTree</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.3, 0.3, 0.3)</volume>
      </shadowData>
    </graphicData>
    <blockWind>false</blockWind>
    <plant>
      <dieIfLeafless Inherit="False">false</dieIfLeafless>
      <growDays>20</growDays>
      <lifespanDaysPerGrowDays>0</lifespanDaysPerGrowDays>
      <harvestedThingDef>AR_RawOlives</harvestedThingDef>
      <harvestTag>Standard</harvestTag>
      <harvestMinGrowth>0.85</harvestMinGrowth>
      <harvestWork>1250</harvestWork>
      <harvestYield>100</harvestYield>
      <wildClusterWeight>50</wildClusterWeight>
      <visualSizeRange>1.3~2.0</visualSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <harvestFailable>false</harvestFailable>
      <fertilityMin>0.3</fertilityMin>
      <wildOrder>2</wildOrder>
      <topWindExposure>0.15</topWindExposure>
      <mustBeWildToSow>false</mustBeWildToSow>
      <immatureGraphicPath>Items/Plants/OlivePlant</immatureGraphicPath>
      <leaflessGraphicPath>Items/Plants/Tree_OliveNotPoplar_Leafless</leaflessGraphicPath>
      <forceIsTree>true</forceIsTree>
      <harvestAfterGrowth>0.2</harvestAfterGrowth>
      <wildBiomes>
        <!-- RimWorld.Core -->
        <!-- VANILLA RENDITION -->
        <AridShrubland>0.2</AridShrubland>
        <TemperateForest>0.03</TemperateForest>
        <Desert>0.1</Desert>
        <ExtremeDesert>0.05</ExtremeDesert>
        <!-- ___________________ -->
        <!-- Mod Biomes -->
        <!-- ___________________ -->
        <!-- sarg.alphabiomes -->
        <!-- ALPHA BIOMES -->
        <AB_GallatrossGraveyard MayRequire="sarg.alphabiomes">0.2</AB_GallatrossGraveyard>
        <AB_GelatinousSuperorganism MayRequire="sarg.alphabiomes">0.1</AB_GelatinousSuperorganism>
        <!-- Same climate except dryer -->
        <AB_MechanoidIntrusion MayRequire="sarg.alphabiomes">0.2</AB_MechanoidIntrusion>
        <!-- Olives look like eyes -->
        <AB_OcularForest MayRequire="sarg.alphabiomes">0.15</AB_OcularForest>
        <AB_PyroclasticConflagration MayRequire="sarg.alphabiomes">0.01</AB_PyroclasticConflagration>
        <AB_RockyCrags MayRequire="sarg.alphabiomes">0.05</AB_RockyCrags>
        <AB_TarPits MayRequire="sarg.alphabiomes">0.1</AB_TarPits>
        <!-- ___________________ -->
        <!-- BiomesTeam.Oasis -->
        <!-- CHROMATIC OASIS -->
        <!-- ___________________ -->
        <!-- Mlie.AdvancedBiomes -->
        <!-- ADVANCED BIOMES -->
        <PoisonForest MayRequire="Mlie.AdvancedBiomes">0.001</PoisonForest>
        <Savanna MayRequire="Mlie.AdvancedBiomes">0.05</Savanna>
        <Volcano MayRequire="Mlie.AdvancedBiomes">0.05</Volcano>
        <Wasteland MayRequire="Mlie.AdvancedBiomes">0.02</Wasteland>
        <!-- ___________________ -->
        <!-- zylle.MoreVanillaBiomes -->
        <!-- More Vanilla Biomes -->
        <ZBiome_AlpineMeadow MayRequire="zylle.MoreVanillaBiomes">0.02</ZBiome_AlpineMeadow>
        <ZBiome_CoastalDunes MayRequire="zylle.MoreVanillaBiomes">0.1</ZBiome_CoastalDunes>
        <ZBiome_Grasslands MayRequire="zylle.MoreVanillaBiomes">0.1</ZBiome_Grasslands>
        <ZBiome_Sandbar_NoBeach MayRequire="zylle.MoreVanillaBiomes">0.15</ZBiome_Sandbar_NoBeach>
        <!-- ___________________ -->
        <!-- Mlie.NaturesPrettySweet -->
        <!-- NATURE IS PRETTY SWEET -->
        <TKKN_VolcanicFlow MayRequire="Mlie.NaturesPrettySweet">0.01</TKKN_VolcanicFlow>
        <TKKN_RedwoodForest MayRequire="Mlie.NaturesPrettySweet">0.01</TKKN_RedwoodForest>
        <TKKN_Grasslands MayRequire="Mlie.NaturesPrettySweet">0.006</TKKN_Grasslands>
        <!-- PRAIRIE -->
        <TKKN_Savanna MayRequire="Mlie.NaturesPrettySweet">0.1</TKKN_Savanna>
        <TKKN_Desert MayRequire="Mlie.NaturesPrettySweet">0.1</TKKN_Desert>
        <!-- SALT FIELDS -->
        <!-- _________________ -->
        <!-- biomesteam.biomesislands -->
        <!-- BIOMES ISLANDS -->
        <BiomesIslands_BorealIsland MayRequire="biomesteam.biomesislands">0.001</BiomesIslands_BorealIsland>
        <BiomesIslands_DesertIsland MayRequire="biomesteam.biomesislands">0.15</BiomesIslands_DesertIsland>
        <BiomesIslands_DesertArchipelago MayRequire="biomesteam.biomesislands">0.15</BiomesIslands_DesertArchipelago>
        <BiomesIslands_TemperateIsland MayRequire="biomesteam.biomesislands">0.007</BiomesIslands_TemperateIsland>
        <BiomesIslands_TemperateArchipelago MayRequire="biomesteam.biomesislands">0.009</BiomesIslands_TemperateArchipelago>
        <BiomesIslands_TundraIsland MayRequire="biomesteam.biomesislands">0.15</BiomesIslands_TundraIsland>
        <!-- ___________________ -->
        <!-- Heis.Edo.Themed.Expansion -->
        <!-- HISTORICALLY ACCURATE WEEABOO MOD -->
        <HSBambooForest MayRequire="Heis.Edo.Themed.Expansion">0.01</HSBambooForest>
        <!-- ___________________ -->
        <!-- reel.frozenforest -->
        <!-- WINTER TAIGA -->
        <!-- __________________ -->
        <!-- sirmashedpotato.morrowrim -->
        <!-- "MORROWIND" MOD -->
        <MorrowRim_Ashlands MayRequire="sirmashedpotato.morrowrim">0.001</MorrowRim_Ashlands>
        <MorrowRim_AshSwamp MayRequire="sirmashedpotato.morrowrim.ashswamp">0.001</MorrowRim_AshSwamp>
        <MorrowRim_VolcanicAshlands MayRequire="sirmashedpotato.morrowrim.volcanicashlands">0.001</MorrowRim_VolcanicAshlands>
        <!-- ___________________ -->
        <!-- pphhyy.gulden -->
        <!-- FOREST OF GOLD -->
        <TemperateGuldenForest MayRequire="pphhyy.gulden">0.005</TemperateGuldenForest>
      </wildBiomes>
      <interferesWithRoof>false</interferesWithRoof>
      <sowResearchPrerequisites>
        <li>AR_Project_OliveTree</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>
