  <ThingDef ParentName="ArtableBedBase">
    <defName>AR_Kliani</defName>
    <label>kline</label>
    <description>A cozy mattress and sheets on a frame for resting.</description>
    <graphicData>
      <texPath>Buildings/Furniture/Beds/Klinai</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(3,4)</drawSize>
      <damageData>
        <rect>(0,0.05,1,1.95)</rect>
      </damageData>
    </graphicData>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>EndTable</li>
          <li>Dresser</li>
        </linkableFacilities>
      </li>
    </comps>
    <statBases>
      <MaxHitPoints>140</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>9</Beauty>
      <WorkToBuild>870</WorkToBuild>
      <Mass>50</Mass>
